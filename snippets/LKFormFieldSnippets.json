{
  "LK Create Input Form Field": {
    "prefix": "LKCreateInputFormField",
    "body": [
      "/** ${1} **/",
      "lkFormItemUtils.renderInputFormItem(",
      "  {",
      "    label: '${1:表单项显示名称}',",
      "  },",
      "  new LkInputConfig({",
      "    placeholder: '请输入${1}'",
      "  }),",
      "  {",
      "    name: '${2:表单项目数据名称}',",
      "    rules: [",
      "      { required: ${3:true}, message: '${1}不能为空', whitespace: ${4:true} }",
      "    ]",
      "  }",
      "),"
    ],
    "description": "LK Create Input Form Field"
  },

