{
  "HL Create Custom From Config": {
    "prefix": "LKCreateCustomFormConfig",
    "body": [
      "lkFormItemUtils.renderCustomFormItem(",
      "  {",
      "     label: \"${1}\",",
      "   },",
      "  {",
      "     renderJSX: () => {",
      "       return (",
      "           <div></div>",
      "       );",
      "     },",
      "   }",
      ")"
    ],
    "description": "渲染自定义表单项配置"
  },
  "LK Create Table Columns Config": {
    "prefix": "LKCreateTableColumnsConfig",
    "body": [
      "{",
      "  title: '${1}',",
      "  dataIndex: '${2}',",
      "  key: '${2}',",
      "},"
    ],
    "description": "渲染表格标头配置"
  },
  "LK Create Table Custom Columns Config": {
    "prefix": "LKCreateTableCustomColumnsConfig",
    "body": [
      "{",
      "  title: '${1}',",
      "  dataIndex: '${2}',",
      "  key: '${2}',",
      "  customRender: (text: string, row: ${3: any}, index: number) => {",
      "    return <span>{text}</span>",
      "  },",
      "},"
    ],
    "description": "渲染自定义表格标头配置"
  }
}
