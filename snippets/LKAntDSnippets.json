{
    "Create LK Components Modal" : {
        "prefix" : "LKComponentsModal",
        "body" : [
            "<LkModalAntdv",
            "   onReady={(modalRef: LkModalInstance) => this.${1}ModalRef = modalRef}",
            "   config={new LkModalConfig({",
            "        title: '${2}',",
            "        okText: '确定',",
            "        cancelText: '取消',",
            "})}",
            ">",
            "</LkModalAntdv>"
        ],
        "description" : "生成弹窗模板"
    },
    "Create LK Components Tabs" : {
        "prefix" : "LKComponentsTabs",
        "body" : [
            "{/** ${1:name} */}",
            "<Tabs",
            "   defaultActiveKey=\"${2:key}\" //当前选中索引",
            "   change={this.${3:callback}} //选中事件",
            ">",
            "   <TabPane key=\"${2}\" tab=\"${3:name}\">",
            "       ${4:title}",
            "   <TabPane>",
            "   <TabPane key=\"${5}\" tab=\"${6:name}\">",
                "       ${7:title}",
                "<TabPane>",
            "</Tabs>",
            "{/** 官方文档位置 https://www.Componentsv.com/components/tabs-cn/ */}"
        ],
        "description" : "生成标签页模板"
    },
    "Create LK Components Grid"  : {
        "prefix" : "LKComponentsGrid",
        "body" : [
            "{/** ${1:name} */}",
            "<Row type=\"flex\">",
            "   <Col flex=\"${2}\"></Col>",
            "   <Col flex=\"${3}\"></Col>",
            "</Row>",
            "{/** 官方文档位置 https://www.Componentsv.com/components/grid-cn/ */}"
        ],
        "description" : "生成删格模板"
    },
    "Create LK Components Table" : {
        "prefix" : "LKComponentsTable",
        "body" : [
            "<LkTableAntdv",
            "   columns={new ${1}ColumnsConfig().config(this)}",
            "   dataSource={this.${1}Data}",
            "   config={new LkTableConfig({})}",
            "   onChange={this.${2}}",
            ">",
            "</LkTableAntdv>"
        ],
        "description" : "生成表格模板"
    }
}