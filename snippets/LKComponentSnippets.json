  },

  "HL Create Print Component": {
    "prefix": "HLComponentPrint",
    "body": [
      "<HLPrint",
      "    copyStyles={true}",
      "    bodyClass=''",
      "    trigger={() => {",
      "        return <Button type=\"primary\" icon=\"printer\">打印</Button>",
      "    }}",
      "    content={() => {",
      "        return document.getElementById('${1:app}')",
      "    }}",
      "    onBeforePrint={() => {}}",
      "    onAfterPrint={() => {}}",
      "    debug={false}",
      "></HLPrint>"
    ],
    "description": "HL Create Print Component"
  }

