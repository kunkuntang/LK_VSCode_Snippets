{
  "name": "lk-vscode-snippets",
  "displayName": "LK_VSCode_Snippets",
  "description": "This is extension for lk-components-antdv",
  "version": "0.0.3",
  "publisher": "kuntang",
  "engines": {
    "vscode": "^1.51.0"
  },
  "categories": [
    "Snippets"
  ],
  "activationEvents": [
    "onCommand:extension.createModel",
    "onCommand:extension.createTask"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.createModel",
        "title": "Lk Create Data Model"
      },
      {
        "command": "extension.createTask",
        "title": "Lk Create A Task"
      }
    ],
    "snippets": [
      {
        "language": "typescriptreact",
        "path": "./snippets/LKComponentSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/LKContainerSnippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/LKContainerSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/HLQueryConditionSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/HLFormItemSnippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/LKFormFieldSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/HKSnippets.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/HKSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/LKAntDSnippets.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/LKConfigSnippets.json"
      }
    ],
    "configuration":[
      {
        "title": "lk-vscode-snippets",
        "properties": {
          "lkVscodeSnippets.taskList": {
            "type":"array",
            "default": "['111']",
            "description": "当前存在的任务列表"
          }
        }
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "compile": "webpack --devtool nosources-source-map --config ./build/node-extension.webpack.config.js",
    "watch": "webpack --watch --devtool nosources-source-map --info-verbosity verbose --config ./build/node-extension.webpack.config.js",
    "package": "webpack --mode production --config ./build/node-extension.webpack.config.js",
    "test-compile": "tsc -p ./",
    "test-watch": "tsc -watch -p ./",
    "pretest": "yarn run test-compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kunkuntang/Lk_VSCode_Snippets"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.0.0",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.51.0",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "eslint": "^7.9.0",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "ts-loader": "^8.0.3",
    "typescript": "^4.0.2",
    "vscode-test": "^1.4.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {}
}
